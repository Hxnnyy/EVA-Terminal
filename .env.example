NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
NEXT_PUBLIC_SITE_URL=https://localhost:3000
NEXT_PUBLIC_SUPABASE_PROJECT_MDX_BUCKET=project-mdx
NEXT_PUBLIC_SUPABASE_REEL_BUCKET=reel
NEXT_PUBLIC_SUPABASE_CV_BUCKET=cv
SUPABASE_SERVICE_ROLE=your-service-role-key
ALPHAVANTAGE_API_KEY=optional-fallback-key
ALPHAVANTAGE_ENDPOINT=https://www.alphavantage.co/query
CV_FALLBACK_URL=
CV_FALLBACK_FILE_NAME=
CV_FALLBACK_LAST_UPDATED=
CV_FALLBACK_SIZE_BYTES=
CV_FALLBACK_CHECKSUM=
INVESTMENTS_FETCH_ENABLED=true
INVESTMENTS_REFRESH_INTERVAL_HOURS=24
CRON_SECRET=
ADMIN_E2E_FIXTURE=false

# Test toggles:
# - Set SUPABASE_DISABLED_FOR_TESTS=true to run E2E/Vitest without real Supabase; fixtures/fallbacks are used.
# - SUPPRESS_ENV_VALIDATION_FOR_TESTS=true relaxes env schema validation for test runs.
# - Playwright defaults to these when not provided; Vitest defaults are set in tests/setup-tests.ts.
SUPABASE_DISABLED_FOR_TESTS=false
SUPPRESS_ENV_VALIDATION_FOR_TESTS=false

# E2E test credentials (Playwright only, not used in production):
# - NON_ADMIN_EMAIL: Email for a non-admin test user (for testing permission denials)
# - NON_ADMIN_PASSWORD: Password for the non-admin test user
# NON_ADMIN_EMAIL=
# NON_ADMIN_PASSWORD=
